pipeline {
  agent any
  tools {}
  stages {
    stage('build') {
      steps {
        sh ''' docker - compose up--build - d  '''
      }
    }
    stage('test') {
      steps {
        sh 'python3 app_test.py'
      }
      post {
        always {
          junit 'test-reports/*.xml'
        }
      }
    }
  }
}